.hasEnrollView{
  align-items: center;
  padding: 10%;
  justify-content: center;
}
.hasEnroll_container{
  display: flex;
  justify-content: center;
  font-size: 20px;
  color: #666;
}
.hasEnroll_context{
  margin-top: 40px;
  display: flex;
  justify-content: center;
}
.time_context,.order_context{
  margin-top: 10px;
  display: flex;
  justify-content: center;
}
.textType{
  color: #666;
  font-size: 20px;
  display: block;
  text-align: center;
}
.timeType,.orderType{
  color: #666;
  font-size: 16px;
  display: block;
  text-align: center;
}
.backBtn{
  background-color: #aaaaaa;
  color: #fff;
  margin-top: 20px;
  width: 60%;
}
.rechargeBtn{
  background-color: #ff6100;
  color: #fff;
  margin-top: 20px;
  width: 60%;
}